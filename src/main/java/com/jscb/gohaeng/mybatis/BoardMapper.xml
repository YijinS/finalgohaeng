<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="board">

	<resultMap type="com.jscb.gohaeng.dto.BoardDto" id="boardDto">
		<result property="index" column="BOARD_INDEX" />
		<result property="subject" column="BOARD_SUBJECT" />
		<result property="content" column="BOARD_CONTENT" />
		<result property="userId" column="BOARD_USERID" />
		<result property="fileName" column="BOARD_FILENAME" />
	</resultMap>
	
	<insert id="insert" parameterType="com.jscb.gohaeng.dto.BoardDto">
		INSERT INTO BOARD
			(SUBJECT, CONTENT, USERID, FILENAME)
		VALUES(#{subject}, #{content}, #{userId}, #{fileName})	
	</insert>
</mapper>